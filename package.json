  "dependencies": {
    "named-regexp-groups": "^1.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "chai": "^4.2.0",
    "del-cli": "^1.1.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "hubot-pretend": "^1.2.0"
