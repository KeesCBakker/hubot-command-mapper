{
  "name": "hubot-command-mapper",
  "version": "4.0.1",
  "description": "Helps with mapping tools and commands to hubot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run mocha --recursive test/**/*.spec.ts --exit",
    "mocha": "mocha -r ts-node/register",
    "build": "del-cli dist & tsc",
    "prepublishOnly": "npm run build & npm test & npm version patch",
    "postpublish": "git push origin --tags",
    "patch": "npx npm-force-resolutions & npm audit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeesCBakker/hubot-command-mapper.git"
  },
  "keywords": [
    "Hubot",
    "command",
    "mapper"
  ],
  "author": "Kees C. Bakker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KeesCBakker/hubot-command-mapper/issues"
  },
  "homepage": "https://github.com/KeesCBakker/hubot-command-mapper#readme",
  "dependencies": {
    "named-regexp-groups": "^1.0.6",
    "remove-markdown": "^0.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/hubot": "^3.3.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.3",
    "chai": "^4.3.7",
    "coffeescript": "^2.7.0",
    "del-cli": "^4.0.1",
    "hubot-pretend": "^1.2.0",
    "mocha": "^10.2.0",
    "npm-force-resolutions": "0.0.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "base64-url": "2.3.2",
    "debug": "3.1.0",
    "fresh": "0.5.2",
    "mime": "1.4.1",
    "minimist": "0.2.1",
    "morgan": "1.9.1",
    "negotiator": "0.6.1",
    "qs": "6.0.4"
  }
}
